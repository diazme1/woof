{
  "name": "woof-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start:react": "cross-env BROWSER=none react-scripts start",
    "start:spring": "node -e \"const {spawn}=require('child_process');const isWin=process.platform==='win32';const cmd=isWin?'gradlew.bat':'./gradlew';const args=['bootRun'];const cp=spawn(cmd,args,{stdio:'inherit',cwd:'..',shell:true});cp.on('exit',c=>process.exit(c));\"",
    "dev": "concurrently -k -n REACT,SPRING -c auto \"npm run start:react\" \"npm run start:spring\"",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "cross-env": "^10.0.0"
  }
}
